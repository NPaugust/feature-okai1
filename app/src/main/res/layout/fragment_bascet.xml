<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.fragment.basket.BascetFragment">


    <TextView
        android:id="@+id/empty_backet_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="282dp"
        android:layout_marginRight="119dp"
        android:layout_marginBottom="324dp"
        android:text="@string/emptybacket"
        android:textColor="@color/black"

        android:textSize="12sp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_backet_catalog"
        android:layout_width="163dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:paddingLeft="20dp"
        android:paddingBottom="12dp"
        android:paddingTop="12dp"
        android:paddingRight="20dp"
        android:layout_marginLeft="99dp"
        android:layout_marginTop="310dp"
        android:layout_marginRight="98dp"
        android:layout_marginBottom="310dp"
        android:background="@drawable/button_shape_circle_basket"
        android:text="@string/btn_backet_katalog"
        android:textAllCaps="false"
        android:textSize="14sp" />


</FrameLayout>